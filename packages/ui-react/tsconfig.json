{
  "$schema": "https://json.schemastore.org/tsconfig",

  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "composite": true,
    "strict": true,
    "noUncheckedIndexedAccess": true
  },
  "include": ["."],
  "exclude": ["node_modules"]
}
